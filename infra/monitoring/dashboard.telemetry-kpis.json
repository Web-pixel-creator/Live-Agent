{
  "displayName": "MLA Telemetry KPI Overview",
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "xPos": 0,
        "yPos": 0,
        "width": 6,
        "height": 4,
        "widget": {
          "title": "P95 Duration by Service (ms)",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/mla_analytics_metric_value\" AND metric.labels.metric_type=monitoring.regex.full_match(\".*duration_ms\")",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95",
                      "crossSeriesReducer": "REDUCE_NONE",
                      "groupByFields": [
                        "metric.labels.service"
                      ]
                    }
                  }
                },
                "plotType": "LINE",
                "legendTemplate": "{{metric.labels.service}} / {{metric.labels.metric_type}}"
              }
            ],
            "yAxis": {
              "label": "ms",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 0,
        "width": 6,
        "height": 4,
        "widget": {
          "title": "Analytics Error Count Rate (per sec)",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/mla_analytics_error_count\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": [
                        "metric.labels.service"
                      ]
                    }
                  }
                },
                "plotType": "LINE",
                "legendTemplate": "{{metric.labels.service}}"
              }
            ],
            "yAxis": {
              "label": "errors/sec",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 0,
        "yPos": 4,
        "width": 12,
        "height": 4,
        "widget": {
          "title": "Analytics Event Errors by Service/Event",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/mla_analytics_event_error_count\"",
                    "aggregation": {
                      "alignmentPeriod": "60s",
                      "perSeriesAligner": "ALIGN_RATE",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": [
                        "metric.labels.service",
                        "metric.labels.event_type"
                      ]
                    }
                  }
                },
                "plotType": "STACKED_AREA",
                "legendTemplate": "{{metric.labels.service}} / {{metric.labels.event_type}}"
              }
            ],
            "yAxis": {
              "label": "events/sec",
              "scale": "LINEAR"
            }
          }
        }
      }
    ]
  }
}
